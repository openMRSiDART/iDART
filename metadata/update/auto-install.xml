<AutomatedInstallation langpack="eng">
    <com.izforge.izpack.panels.HelloPanel/>
    <com.izforge.izpack.panels.HTMLInfoPanel/>
    <com.izforge.izpack.panels.HTMLLicencePanel/>
    <com.izforge.izpack.panels.PacksPanel>
        <pack name="Core" index="0" selected="true"/>
        <pack name="Libraries" index="1" selected="true"/>
        <pack name="Source" index="2" selected="true"/>
        <pack name="Documentation" index="3" selected="true"/>
    </com.izforge.izpack.panels.PacksPanel>
    <com.izforge.izpack.panels.UserInputPanel>
        <userInput>
            <entry key="isEkapaVersion" value="false"/>
            <entry key="dbPassword" value="postgres"/>
            <entry key="dbName" value="pharm"/>
            <entry key="dbUser" value="postgres"/>
            <entry key="dbAddress" value="localhost"/>
        </userInput>
    </com.izforge.izpack.panels.UserInputPanel>
    <com.izforge.izpack.panels.UserInputPanel>
        <userInput/>
    </com.izforge.izpack.panels.UserInputPanel>
    <com.izforge.izpack.panels.UserInputPanel>
        <userInput>
            <entry key="PatientNameOnReports" value="true"/>
            <entry key="DispenseDirectlyDefault" value="true"/>
            <entry key="PatientNameOnDrugLabel" value="true"/>
        </userInput>
    </com.izforge.izpack.panels.UserInputPanel>
    <com.izforge.izpack.panels.UserInputPanel>
        <userInput>
            <entry key="PrintSideTreatmentLabels" value="true"/>
            <entry key="intValueOfAlternativeBarcodeEndChar" value="-1"/>
            <entry key="accumByDefault" value="true"/>
            <entry key="labelType" value="iDART"/>
            <entry key="SummaryLabelDefault" value="false"/>
            <entry key="patientBarcodeRegex" value=".+"/>
        </userInput>
    </com.izforge.izpack.panels.UserInputPanel>
    <com.izforge.izpack.panels.UserInputPanel>
        <userInput>
            <entry key="nextAppointmentDateOnLabels" value="true"/>
            <entry key="logoutTime" value="600"/>
        </userInput>
    </com.izforge.izpack.panels.UserInputPanel>
    <com.izforge.izpack.panels.TargetPanel>
                <installpath>@install.path@</installpath>
    </com.izforge.izpack.panels.TargetPanel>
    <com.izforge.izpack.panels.ShortcutPanel/>
    <com.izforge.izpack.panels.InstallPanel/>
    <com.izforge.izpack.panels.ProcessPanel/>
    <com.izforge.izpack.panels.FinishPanel/>
</AutomatedInstallation>